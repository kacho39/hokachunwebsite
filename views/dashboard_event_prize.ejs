<!DOCTYPE html>
<html>
<head>
	<title>Dashboard</title>

	<%- include('partials/head'); %>

</head>
<body>
	<%- include('partials/nav'); %>
	<div class="container">
		<div class="row">
			<% prize_id_array.forEach(function(prize) { %>
				
				<div class="col-sm-3">
					<!-- <%= prize.prize_id %> -->
					<div class="event-prize-div">
						<img class="event-prize-img" src="/images/product/prize_<%= prize.prize_id %>.png">
					</div>
				</div>
			<% }); %>
		</div>
		<div class="row">
			<div class="col-sm-12">
				<a href="/dashboard" class="btn btn-default" role="button">
					Back
				</a>
			</div>
		</div>
	</div>
</body>

</html>