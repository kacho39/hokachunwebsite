<!DOCTYPE html>
<html>
<head>
	<title>Dashboard</title>

	<%- include('partials/head'); %>

	<style type="text/css">
		.checkbox-size-20 {
			width: 20px;
			height: 20px;
		}
	</style>

</head>
<body>
	<%- include('partials/nav'); %>
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<div>
					<% if(prize_token_array.length > 0){ %>
						<table class="table">
							<thead>
								<tr>
									<th scope="col">ID</th>
									<th scope="col">Scratched at</th>
								</tr>
							</thead>
							<tbody>
								<% prize_token_array.forEach(function(prize_token) { %>
									<tr>
										<td> <%= prize_token.id %> </td>
										<td> <%= prize_token.scratched_at %> </td>
									</tr>
								<% }); %>
							</tbody>
						</table>
			 		<% } else { %>
						<h3 style="margin-top: 20px;text-align: center;">No event for this account</h3>
			 		<% } %>

				</div>
			</div>
		</div>
	</div>
</body>

</html>